@model IEnumerable<WebApplication2.Models.RssNews>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Home Page</h2>


@foreach (var item in Model)
{
    <br>
    <br />

        <ul>
            <li>Title : @item.Title</li>
            <li>@(new HtmlString(@item.Text)) </li>
            <li>Date : @item.PublicationDate </li>
            <li>Link : @item.Link <a href=@item.Link target="_blank"> open article</a></li>
            <li>Source : @item.Source </li>
            @(new HtmlString(@item.Category_html))
            @(new HtmlString(@item.Image))
        </ul>

}




